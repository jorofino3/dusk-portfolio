			h6.section-fullpage-title contacts
			.section-fullpage-inner
				.container.text-center
					.row.justify-content-center
						.col-lg-9.col-xl-7
							p.big Tell me about your story, your passion, your love.
								br
								| And I'll be happy to get back to you and talk about our common project. Let's connect here.
							h3.text-spacing-21: a( href='tel:#' ) +1 256 10 87 000
							h5.mt-2.font-weight-light 4401 Waldeck Street Grapevine, Nashville, TX 76051
							a.mail-link( href='mailto:#' ) info@demolink.org
			.parallax-scene
				.parallax-scene-item.parallax-scene-item-7.d-none.d-lg-block: .layer( data-depth='.2' ): +img( 'home-10-473x345' ).tilter
				.parallax-scene-item.parallax-scene-item-10: .layer( data-depth='-.36' ): +img( 'home-11-398x290' ).tilter
				.parallax-scene-item.parallax-scene-item-9: .layer( data-depth='.2' ): +img( 'home-12-252x184' ).tilter
	include ../sections/_footer-classic
