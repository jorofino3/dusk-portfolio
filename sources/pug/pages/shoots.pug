			.container.text-center
				.row.justify-content-center
					.col-12.col-xl-9.col-xxl-12
						.row.row-15.row-md-30.gutters-10.gutters-md-30( data-lightgallery='group' )
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/wedding-1-820x598.jpg' data-lightgallery='item' )
										+img( 'wedding-1-400x292' )
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/wedding-2-820x598.jpg' data-lightgallery='item' )
										+img( 'wedding-2-400x292' )
										
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/wedding-3-820x598.jpg' data-lightgallery='item' )
										+img( 'wedding-3-400x292' )
										
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/wedding-4-820x598.jpg' data-lightgallery='item' )
										+img( 'wedding-4-400x292' )
										
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/wedding-5-820x598.jpg' data-lightgallery='item' )
