extends ../_skeleton

block variables
	- pageName = 'Fashion'

block page
	include ../sections/_header-classic
	
	section.section.section-fullpage.section-md.bg-gray-100 
		h6.section-fullpage-title FASHION
		.section-fullpage-inner
			.container.text-center
				.row.justify-content-center
					.col-12.col-xl-9.col-xxl-12
						.row.row-15.row-md-30.gutters-10.gutters-md-30( data-lightgallery='group' )
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/fashion-1-820x598.jpg' data-lightgallery='item' )
										+img( 'fashion-1-400x292' )
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/fashion-2-820x598.jpg' data-lightgallery='item' )
										+img( 'fashion-2-400x292' )
										
							.col-6.col-lg-4
