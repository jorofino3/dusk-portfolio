									a.figure-classic( href='images/fashion-3-820x598.jpg' data-lightgallery='item' )
										+img( 'fashion-3-400x292' )
										
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/fashion-4-820x598.jpg' data-lightgallery='item' )
										+img( 'fashion-4-400x292' )
										
