			.container.text-center
				.row.justify-content-center
					.col-12.col-xl-9.col-xxl-12
						.row.row-15.row-md-30.gutters-10.gutters-md-30( data-lightgallery='group' )
							.col-6.col-lg-4
								.tilter
									a.figure-classic( href='images/motion-1-820x598.jpg' data-lightgallery='item' )
										+img( 'motion-1-400x292' )
